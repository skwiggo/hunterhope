<!-- Scaffolding generated by Casein v5.1.1.5 -->

<table class="table table-striped table-hover table-condensed">
	<thead>
		<tr>
			<th><%= casein_sort_link "Venue", :venue %></th>
			<th><%= casein_sort_link "Lat", :lat %></th>
			<th><%= casein_sort_link "Long", :long %></th>
			<th><%= casein_sort_link "Otherbands", :otherbands %></th>
			<th><%= casein_sort_link "Date", :date %></th>
			<th>&nbsp;</th>
		</tr>
	</thead>
	
	<% tourdates.each do |tourdate| %>
		<tr>
			<td><%= casein_table_cell_link tourdate.venue, casein_tourdate_path(tourdate) %></td>
			<td><%= casein_table_cell_link tourdate.lat, casein_tourdate_path(tourdate) %></td>
			<td><%= casein_table_cell_link tourdate.long, casein_tourdate_path(tourdate) %></td>
			<td><%= casein_table_cell_link tourdate.otherbands, casein_tourdate_path(tourdate) %></td>
			<td><%= casein_table_cell_link tourdate.date, casein_tourdate_path(tourdate) %></td>
			<td class="delete">
				<%= link_to(casein_show_row_icon("trash"), casein_tourdate_path(tourdate), :method => :delete, :data => { :confirm => "Are you sure you want to delete this tourdate?" }) %>
			</td>
		</tr>
	<% end %>
</table>